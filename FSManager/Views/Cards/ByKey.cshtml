@model FSManager.Dto.Cards.GetCard

@{
    ViewData["Title"] = $"Card {Model.Name}";
}

<h2>@Model.Name</h2>
<div class="container">
    <div class="row">
        <div class="col">
            <h3>Text</h3>
            <p>@Model.Text</p>
            <p>@Model.Script</p>
        </div>
        <div class="col">
            <img src="@Model.ImageUrl" alt="-" class="img-fluid" style="width: 320px">
        </div>
    </div>

</div>